<generatorConfiguration>
<!--    <classPathEntry location="H:\TestProjects\小程序\mysql-connector-java-5.1.34.jar"/>-->
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!--<plugin type="generator.plugins.CachePlugin"></plugin>-->
        <plugin type="generator.plugins.SerializablePlugin"></plugin>
        <plugin type="generator.plugins.MySQLPagePlugin"></plugin>
        <plugin type="generator.plugins.MySQLFixedPlugin"></plugin>
        <commentGenerator>
            <!-- 去除自动生成的注释 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://139.196.160.176:3306/applet_site_test?characterEncoding=utf8&amp;useSSL=false&amp;serverTimezone=UTC&amp;rewriteBatchedStatements=true"
                        userId="root"
                        password="ml-mysql">
            <property name="useInformationSchema" value="true"/>
        </jdbcConnection>
        <javaTypeResolver type="generator.plugins.JavaTypeResolverForMysql">
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!-- targetProject:生成PO类的位置 -->
        <javaModelGenerator targetPackage="com.applet.user.entity"
                            targetProject="user\src\main\java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
            <!-- 从数据库返回的值被清理前后的空格 -->
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!-- targetProject:mapper映射文件生成的位置 -->
        <sqlMapGenerator targetPackage="com.applet.user.mapper"
                         targetProject="user\src\main\resources">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>
        <!-- targetPackage：mapper接口生成的位置 -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.applet.user.mapper"
                             targetProject="user\src\main\java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>

